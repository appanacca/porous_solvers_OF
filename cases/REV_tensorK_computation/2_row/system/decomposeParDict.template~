FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains |- nproc -|;


<!--(if D2)-->
method		scotch;
<!--(elif D3)-->
method          simple;
<!--(end)-->

preservePatches (front back);

simpleCoeffs
{
    n               (1 1 |- nproc -|);
    delta           0.001;
}

hierarchicalCoeffs
{
    n               ( 1 1 4 );
    delta           0.001;
    order           xyz;
}

manualCoeffs
{
    dataFile        "";
}

distributed     no;

roots           ( );


